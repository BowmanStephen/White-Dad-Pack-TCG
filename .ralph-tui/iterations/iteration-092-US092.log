  // User closed a modal\n   388→  | 'achievement_unlock'  // User unlocked an achievement (US073)\n   389→  | 'daily_reward_claim'  // User claimed daily reward (US074)\n   390→  | 'trade_create'        // User created a trade offer (US078)\n   391→  | 'trade_accept'        // User accepted a trade (US078)\n   392→  | 'trade_reject'        // User rejected a trade (US078)\n   393→  | 'trade_cancel';       // User cancelled a trade (US078)\n   394→\n   395→// Share platforms\n   396→export type SharePlatform = 'twitter' | 'discord' | 'download' | 'native' | 'copy_link';\n   397→\n   398→// Base analytics event interface\n   399→export interface AnalyticsEvent {\n   400→  id: string;                 // Unique event ID\n   401→  type: AnalyticsEventType;   // Event type\n   402→  timestamp: number;          // Unix timestamp (ms)\n   403→  sessionId: string;          // Session identifier\n   404→}\n   405→\n   406→// Pack open event\n   407→export interface PackOpenEvent extends AnalyticsEvent {\n   408→  type: 'pack_open';\n   409→  data: {\n   410→    packId: string;           // Pack identifier\n   411→    cardCount: number;        // Number of cards in pack\n   412→  };\n   413→}\n   414→\n   415→// Card reveal event\n   416→export interface CardRevealEvent extends AnalyticsEvent {\n   417→  type: 'card_reveal';\n   418→  data: {\n   419→    packId: string;           // Pack identifier\n   420→    cardIndex: number;        // Position in pack (0-based)\n   421→    cardId: string;           // Card identifier (generic, no PII)\n   422→    rarity: Rarity;           // Card rarity\n   423→    isHolo: boolean;          // Whether card is holographic\n   424→    holoType: HoloVariant;    // Holo variant type\n   425→    autoRevealed: boolean;    // Whether auto-revealed or manual\n   426→  };\n   427→}\n   428→\n   429→// Pack complete event\n   430→export interface PackCompleteEvent extends AnalyticsEvent {\n   431→  type: 'pack_complete';\n   432→  data: {\n   433→    packId: string;           // Pack identifier\n   434→    cardCount: number;        // Number of cards in pack\n   435→    bestRarity: Rarity;       // Best rarity pulled\n   436→    holoCount: number;        // Number of holo cards\n   437→    duration: number;         // Time to complete (ms)\n   438→    skipped: boolean;         // Whether user skipped animation\n   439→  };\n   440→}\n   441→\n   442→// Share event\n   443→export interface ShareEvent extends AnalyticsEvent {\n   444→  type: 'share';\n   445→  data: {\n   446→    platform: SharePlatform;  // Platform shared to\n   447→    packId: string;           // Pack identifier (if applicable)\n   448→    cardCount: number;        // Number of cards shared\n   449→  };\n   450→}\n   451→\n   452→// Collection view event\n   453→export interface CollectionViewEvent extends AnalyticsEvent {\n   454→  type: 'collection_view';\n   455→  data: {\n   456→    totalPacks: number;       // Total packs in collection\n   457→    totalCards: number;       // Total cards in collection\n   458→    uniqueCards: number;      // Unique cards in collection\n   459→  };\n   460→}\n   461→\n   462→// Collection filter event\n   463→export interface CollectionFilterEvent extends AnalyticsEvent {\n   464→  type: 'collection_filter';\n   465→  data: {\n   466→    filterType: string;       // Type of filter (rarity, dad type, etc.)\n   467→    filterValue: string;      // Filter value applied\n   468→  };\n   469→}\n   470→\n   471→// Collection sort event\n   472→export interface CollectionSortEvent extends AnalyticsEvent {\n   473→  type: 'collection_sort';\n   474→  data: {\n   475→    sortOption: SortOption;   // Sort option selected\n   476→  };\n   477→}\n   478→\n   479→// Settings open event\n   480→export interface SettingsOpenEvent extends AnalyticsEvent {\n   481→  type: 'settings_open';\n   482→  data: {\n   483→    source: 'button' | 'keyboard'; // How settings were opened\n   484→  };\n   485→}\n   486→\n   487→// Settings change event\n   488→export interface SettingsChangeEvent extends AnalyticsEvent {\n   489→  type: 'settings_change';\n   490→  data: {\n   491→    setting: string;          // Setting that changed (e.g., 'soundEnabled')\n   492→    previousValue: boolean | number | string;  // Previous value\n   493→    newValue: boolean | number | string;        // New value\n   494→  };\n   495→}\n   496→\n   497→// Modal open event\n   498→export interface ModalOpenEvent extends AnalyticsEvent {\n   499→  type: 'modal_open';\n   500→  data: {\n   501→    modalType: 'card_inspection' | 'share' | 'settings'; // Type of modal\n   502→    trigger: 'click' | 'keyboard'; // How modal was opened\n   503→  };\n   504→}\n   505→\n   506→// Modal close event\n   507→export interface ModalCloseEvent extends AnalyticsEvent {\n   508→  type: 'modal_close';\n   509→  data: {\n   510→    modalType: 'card_inspection' | 'share' | 'settings'; // Type of modal\n   511→    duration: number;         // Time open (ms)\n   512→  };\n   513→}\n   514→\n   515→// Achievement unlock event (US073)\n   516→export interface AchievementUnlockEvent extends AnalyticsEvent {\n   517→  type: 'achievement_unlock';\n   518→  data: {\n   519→    achievementId: string;    // Achievement identifier\n   520→    achievementName: string;  // Achievement name\n   521→    rarity: string;           // Achievement rarity (bronze, silver, gold, platinum)\n   522→    category: string;         // Achievement category (opening, collection, rare, holo, variety)\n   523→  };\n   524→}\n   525→\n   526→// Daily reward claim event (US074)\n   527→export interface DailyRewardClaimEvent extends AnalyticsEvent {\n   528→  type: 'daily_reward_claim';\n   529→  data: {\n   530→    rewardType: string;       // Type of reward claimed (pack, boosted_pack, cards, currency)\n   531→    rewardValue: number;      // Value of reward\n   532→    streak: number;           // Current streak when claimed\n   533→    bonusMultiplier: number;  // Bonus multiplier applied\n   534→  };\n   535→}\n   536→\n   537→// Union type of all analytics events\n   538→export type AnyAnalyticsEvent =\n   539→  | PackOpenEvent\n   540→  | CardRevealEvent\n   541→  | PackCompleteEvent\n   542→  | ShareEvent\n   543→  | CollectionViewEvent\n   544→  | CollectionFilterEvent\n   545→  | CollectionSortEvent\n   546→  | SettingsOpenEvent\n   547→  | SettingsChangeEvent\n   548→  | ModalOpenEvent\n   549→  | ModalCloseEvent\n   550→  | AchievementUnlockEvent\n   551→  | DailyRewardClaimEvent;\n   552→\n   553→// Analytics configuration\n   554→export interface AnalyticsConfig {\n   555→  enabled: boolean;           // Master switch for analytics\n   556→  providers: {\n   557→    googleAnalytics?: {\n   558→      measurementId: string;  // GA4 measurement ID (e.g., 'G-XXXXXXXXXX')\n   559→      enabled: boolean;\n   560→    };\n   561→    plausible?: {\n   562→      domain: string;         // Plausible domain (e.g., 'daddieck.com')\n   563→      enabled: boolean;\n   564→    };\n   565→  };\n   566→  debugMode: boolean;         // Log events to console in development\n   567→  batchSize: number;          // Number of events to batch before sending\n   568→  flushInterval: number;      // Max time (ms) to hold events before flushing\n   569→}\n   570→\n   571→// Analytics provider interface\n   572→export interface AnalyticsProvider {\n   573→  name: string;\n   574→  initialized: boolean;\n   575→  initialize(config: AnalyticsConfig): Promise<void>;\n   576→  trackEvent(event: AnyAnalyticsEvent): Promise<void>;\n   577→  flush(): Promise<void>;\n   578→}\n   579→\n   580→// Analytics event queue (for batching)\n   581→export interface EventQueue {\n   582→  events: AnyAnalyticsEvent[];\n   583→  lastFlush: number;\n   584→}\n   585→\n   586→// Analytics session data\n   587→export interface AnalyticsSession {\n   588→  id: string;                 // Session identifier\n   589→  startTime: number;          // Session start timestamp\n   590→  pageCount: number;          // Pages viewed in session\n   591→  lastActivity: number;       // Last activity timestamp\n   592→}\n   593→\n   594→// ============================================================================\n   595→// ACHIEVEMENT TYPES (US073 - Achievement System - Milestones)\n   596→// ============================================================================\n   597→\n   598→// Achievement rarity levels\n   599→export type AchievementRarity = 'bronze' | 'silver' | 'gold' | 'platinum';\n   600→\n   601→// Achievement categories\n   602→export type AchievementCategory = 'opening' | 'collection' | 'rare' | 'holo' | 'variety' | 'daily';\n   603→\n   604→// Achievement interface\n   605→export interface Achievement {\n   606→  id: string;\n   607→  name: string;\n   608→  description: string;\n   609→  icon: string;\n   610→  rarity: AchievementRarity;\n   611→  category: AchievementCategory;\n   612→  unlockedAt?: Date;\n   613→  progress?: number;\n   614→  maxProgress?: number;\n   615→}\n   616→\n   617→// Achievement state for tracking\n   618→export interface AchievementState {\n   619→  achievements: Achievement[];\n   620→  recentlyUnlocked: string[]; // IDs of recently unlocked achievements (for popup queue)\n   621→}\n   622→\n   623→// Achievement context for checking conditions\n   624→export interface AchievementContext {\n   625→  pack: Pack | null;\n   626→  collection: Collection;\n   627→  stats: CollectionStats;\n   628→}\n   629→\n   630→// Achievement configuration\n   631→export interface AchievementConfig {\n   632→  name: string;\n   633→  description: string;\n   634→  icon: string;\n   635→  rarity: AchievementRarity;\n   636→  category: AchievementCategory;\n   637→  maxProgress?: number;\n   638→  checkCondition: (context: AchievementContext) => boolean;\n   639→  getProgress?: (context: AchievementContext) => number;\n   640→}\n   641→\n   642→// Achievement rarity configuration (for visual effects)\n