'cards' | 'currency';\n   699→\n   700→// Daily reward interface\n   701→export interface DailyReward {\n   702→  day: number;                  // Day number in current streak (1-based)\n   703→  claimed: boolean;             // Whether reward has been claimed\n   704→  claimedAt?: Date;             // When reward was claimed\n   705→  rewardType: DailyRewardType;  // Type of reward\n   706→  rewardValue: number;          // Value (e.g., pack count, card count)\n   707→  bonusMultiplier?: number;     // Streak bonus multiplier (1.0 = normal)\n   708→}\n   709→\n   710→// Daily rewards state\n   711→export interface DailyRewardsState {\n   712→  currentStreak: number;        // Current login streak (days)\n   713→  longestStreak: number;        // Longest streak achieved\n   714→  lastLoginDate: Date | null;   // Last login date (for streak calculation)\n   715→  nextClaimTime: Date;          // When next reward can be claimed\n   716→  totalClaimed: number;         // Total rewards claimed all-time\n   717→  rewards: DailyReward[];       // Current streak rewards\n   718→  consecutiveDays: number;      // Days in current streak\n   719→  missedDay: boolean;           // Whether user missed a day (resets streak)\n   720→}\n   721→\n   722→// Daily reward tier configuration\n   723→export interface DailyRewardTier {\n   724→  day: number;                  // Day in streak\n   725→  rewardType: DailyRewardType;  // Type of reward\n   726→  baseValue: number;            // Base reward value\n   727→  description: string;          // Reward description\n   728→  icon: string;                 // Icon/emoji\n   729→}\n   730→\n   731→// Streak bonus configuration\n   732→export interface StreakBonus {\n   733→  threshold: number;            // Streak length required\n   734→  multiplier: number;           // Bonus multiplier\n   735→  description: string;          // Bonus description\n   736→}\n   737→\n   738→// ============================================================================\n   739→// ADVANCED SEARCH TYPES (US077 - Card Search - Advanced Filters)\n   740→// ============================================================================\n   741→\n   742→// Stat range filter interface (min-max for each stat)\n   743→export interface StatRangeFilter {\n   744→  min: number;\n   745→  max: number;\n   746→}\n   747→\n   748→// All stat ranges for filtering\n   749→export interface StatRanges {\n   750→  dadJoke?: StatRangeFilter;\n   751→  grillSkill?: StatRangeFilter;\n   752→  fixIt?: StatRangeFilter;\n   753→  napPower?: StatRangeFilter;\n   754→  remoteControl?: StatRangeFilter;\n   755→  thermostat?: StatRangeFilter;\n   756→  sockSandal?: StatRangeFilter;\n   757→  beerSnob?: StatRangeFilter;\n   758→}\n   759→\n   760→// Advanced search filters interface\n   761→export interface AdvancedSearchFilters {\n   762→  searchTerm: string;\n   763→  rarity: Rarity | null;\n   764→  holoVariants: Set<HoloVariant>; // Multiple holo variants (not just \"holo only\")\n   765→  selectedTypes: Set<DadType>;\n   766→  statRanges: StatRanges;\n   767→}\n   768→\n   769→// Saved search preset interface\n   770→export interface SavedSearchPreset {\n   771→  id: string;\n   772→  name: string;\n   773→  description: string;\n   774→  icon: string;\n   775→  filters: AdvancedSearchFilters;\n   776→}\n   777→\n   778→// Holographic variant display names\n   779→export const HOLO_VARIANT_NAMES: Record<HoloVariant, string> = {\n   780→  none: 'Non-Holo',\n   781→  standard: 'Standard Holo',\n   782→  reverse: 'Reverse Holo',\n   783→  full_art: 'Full Art Holo',\n   784→  prismatic: 'Prismatic Holo',\n   785→};\n   786→\n   787→// Holographic variant icons\n   788→export const HOLO_VARIANT_ICONS: Record<HoloVariant, string> = {\n   789→  none: '📄',\n   790→  standard: '✨',\n   791→  reverse: '🌈',\n   792→  full_art: '🖼️',\n   793→  prismatic: '💎',\n   794→};\n   795→\n   796→// ============================================================================\n   797→// TRADE OFFER TYPES (US078 - Card Trading - Trade Offer System)\n   798→// ============================================================================\n   799→\n   800→// Trade offer status\n   801→export type TradeStatus = 'pending' | 'accepted' | 'rejected' | 'cancelled';\n   802→\n   803→// Trade offer interface\n   804→export interface TradeOffer {\n   805→  id: string;                      // Unique trade ID\n   806→  createdAt: Date;                 // When trade was created\n   807→  status: TradeStatus;             // Current status\n   808→  senderName: string;              // Name of sender (user-provided)\n   809→  offeredCards: TradeCard[];       // Cards being offered\n   810→  requestedCards: TradeCard[];     // Cards being requested\n   811→  expiresAt: Date;                 // Trade expiration (7 days)\n   812→  message?: string;                // Optional message from sender\n   813→}\n   814→\n   815→// Card in a trade offer (minimal data for sharing)\n   816→export interface TradeCard {\n   817→  id: string;                      // Card ID\n   818→  name: string;                    // Card name\n   819→  rarity: Rarity;                 // Card rarity\n   820→  type: DadType;                   // Dad type\n   821→  holoVariant: HoloVariant;        // Holo variant\n   822→  isHolo: boolean;                 // Whether holo\n   823→}\n   824→\n   825→// Trade history entry (completed trades)\n   826→export interface TradeHistoryEntry {\n   827→  id: string;                      // Trade ID\n   828→  completedAt: Date;               // When trade was completed\n   829→  partnerName: string;             // Name of trading partner\n   830→  givenCards: TradeCard[];         // Cards you gave\n   831→  receivedCards: TradeCard[];      // Cards you received\n   832→  status: 'accepted' | 'rejected'; // Final status\n   833→}\n   834→\n   835→// Trade state for UI\n   836→export interface TradeState {\n   837→  currentTrade: TradeOffer | null; // Trade being created/viewed\n   838→  sentTrades: TradeOffer[];        // Trades you've sent\n   839→  receivedTrades: TradeOffer[];    // Trades you've received\n   840→  tradeHistory: TradeHistoryEntry[]; // Completed trades\n   841→}\n   842→\n   843→// Trade configuration\n   844→export interface TradeConfig {\n   845→  maxCardsPerSide: number;         // Max cards you can offer/request\n   846→  tradeExpirationDays: number;     // Days before trade expires\n   847→  maxActiveTrades: number;         // Max active trades at once\n   848→}\n   849→\n   850→// Default trade configuration\n   851→export const DEFAULT_TRADE_CONFIG: TradeConfig = {\n   852→  maxCardsPerSide: 6,              // Max 6 cards per side\n   853→  tradeExpirationDays: 7,          // 7 days to expire\n   854→  maxActiveTrades: 10,             // Max 10 active trades\n   855→};\n   856→\n   857→// ============================================================================\n   858→// CRAFTING TYPES (US080 - Card Crafting - Combine Cards)\n   859→// ============================================================================\n   860→\n   861→// Crafting state machine\n   862→export type CraftingState =\n   863→  | 'idle'            // Ready to start crafting\n   864→  | 'selecting'       // Selecting cards for crafting\n   865→  | 'crafting'        // Crafting animation playing\n   866→  | 'success'         // Craft succeeded\n   867→  | 'failed'          // Craft failed\n   868→  | 'history';        // Viewing crafting history\n   869→\n   870→// Crafting recipe interface\n   871→export interface CraftingRecipe {\n   872→  id: string;                      // Unique recipe ID\n   873→  name: string;                    // Recipe display name\n   874→  description: string;             // Recipe description\n   875→  inputRarity: Rarity;             // Input card rarity\n   876→  inputCount: number;              // Number of cards required (typically 5)\n   877→  outputRarity: Rarity;            // Output card rarity\n   878→  outputCount: number;             // Number of cards produced (typically 1)\n   879→  successRate: number;             // Success rate (0-1)\n   880→  failReturnRate?: number;         // On fail, % of cards returned (0-1)\n   881→}\n   882→\n   883→// Crafting session interface\n   884→export interface CraftingSession {\n   885→  id: string;                      // Unique session ID\n   886→  recipe: CraftingRecipe;          // Selected recipe\n   887→  selectedCards: string[];         // IDs of selected cards\n   888→  status: CraftingState;           // Current crafting state\n   889→  result?: PackCard;               // Result card (if successful)\n   890→  timestamp: Date;                 // When crafting started\n   891→  completedAt?: Date;              // When crafting completed\n   892→}\n   893→\n   894→// Crafting history entry\n   895→export interface CraftingHistoryEntry {\n   896→  id: string;                      // Unique entry ID\n   897→  sessionId: string;               // Reference to crafting session\n   898→  recipe: CraftingRecipe;          // Recipe used\n   899→  inputCards: string[];            // Card IDs used as input\n   900→  result?: PackCard;               // Result card (if successful)\n   901→  success: boolean;                // Whether crafting succeeded\n   902→  timestamp: Date;                 // When crafting occurred\n   903→}\n   904→\n   905→// Crafting history state\n   906→export interface CraftingHistory {\n   907→  entries: CraftingHistoryEntry[];  // All crafting attempts\n   908→  totalAttempts: number;            // Total crafts attempted\n   909→  successfulCrafts: number;         // Successful crafts\n   910→  failedCrafts: number;             // Failed crafts\n   911→  bestCraft: PackCard | null;       // Best card crafted\n   912→}\n   913→\n   914→// Crafting configuration\n   915→export interface CraftingConfig {\n   916→  maxHistoryEntries: number;        // Max history 