# Discord Bot Configuration
# Copy this file to .env and fill in your values

# Discord Bot Token (from Discord Developer Portal)
DISCORD_TOKEN=your_bot_token_here

# Discord Client ID (for slash command registration)
DISCORD_CLIENT_ID=your_client_id_here

# Optional: Guild ID for development (commands register instantly)
DISCORD_GUILD_ID=your_guild_id_here

# Cooldown Settings
COOLDOWN_MINUTES=60
PREMIUM_BYPSS_COOLDOWN=true

# Database Path (for user accounts and cooldowns)
DATABASE_PATH=./discord-bot/data/users.json
